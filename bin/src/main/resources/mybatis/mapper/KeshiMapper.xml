<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper
	namespace="com.woniuxy.springboot.HIS.mapper.KeshiMapper">
	<!-- 查询门诊的所有科室 -->
	<select id="selectnoInKeshi" resultType="Keshi">
	 select * from keshi where isin = 1
	</select>
	<select id="selectKeshiByKid" resultType="Keshi">
	select * from keshi where kid = #{kid}
	</select>
</mapper>